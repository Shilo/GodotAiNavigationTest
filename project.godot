config_version=5

[application]

config/name="AI Navigation Test"
config/description="Showcase tilemap navigation mesh and agents.
Agents will follow the mouse position while avoiding walls and each other."
run/main_scene="res://main/main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
